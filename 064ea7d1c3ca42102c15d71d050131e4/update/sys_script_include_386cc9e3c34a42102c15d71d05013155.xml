        <name>filterStudents</name>
        <script><![CDATA[var filterStudents = Class.create();
filterStudents.prototype = {
    initialize: function() {
    },

	getFilteredList:function(){
		var studGr=new GlideRecord('x_1386549_studentm_student');
		studGr.addEncodedQuery('status=registered^ORstatus=suspended^ORstatus=new_registration');
		studGr.query();
		
		arr=[];

		while(studGr.next()){
			arr.push(studGr.getUniqueValue());
		}

		return "sys_idIN"+arr;

	},
	

    type: 'filterStudents'
};]]></script>
